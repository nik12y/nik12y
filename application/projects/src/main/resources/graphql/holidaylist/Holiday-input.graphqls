input HolidayInput {
    holidayId:Int
    holidayName: String  @Size(min: 1, max: 100, message: "holidayName.message") @Pattern(regexp: ".[A-Za-z0-9 .,–()/=‘+:?!”%&*<>;{@#_]{1,100}",message: "Holiday name Invalid pattern")
    holidayType: String  @Size(min: 1, max: 100, message: "holidayType.message") @Pattern(regexp: ".[A-Za-z0-9 .,–()/=‘+:?!”%&*<>;{@#_]{1,100}",message: "Holiday Type Invalid pattern")
    holidayDate: String
}