  description : String! @Size(min: 1, max: 35, message: "description.message") @Pattern(regexp: ".[-A-Za-z0-9 +=]{1,35}",message: "Description Invalid pattern")
    action : String!
    status : String!
    recordVersion : Int!
    createdBy: String
    creationTime: String
    lastUpdatedBy: String
    lastUpdatedTime: String
}