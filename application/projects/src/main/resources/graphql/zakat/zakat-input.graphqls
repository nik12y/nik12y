input ZakatInput{
    zakatYear : Int! @Size(min: 4, max: 4, message: "zakatYear.message") @Pattern(regexp: ".[0-9]{4,4}",message: "Zakat Year Invalid pattern")
    startDateOfRamadan : String
    action : String!
    status : String!
    recordVersion : Int!
    createdBy: String
    creationTime: String
    lastUpdatedBy: String
    lastUpdatedTime: String
}