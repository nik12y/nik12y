input AmountInput{

    countryCode: String! @Size(min: 2, max: 2, message: "countryCode.message") @Pattern(regexp: "[A-Z]+",message: "Country code should be uppercase alphabet of size 2")
    amount: String! @Size(min: 1, max: 40, message: "amount.message") @Pattern(regexp: "[0-9]+",message: "Amount should be of size upto 40")

}